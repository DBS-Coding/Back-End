    "@types/node": "^22.15.29",
    "@types/pg": "^8.15.4",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "supabase": "^2.24.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript-eslint": "^8.31.0"
  },
  "peerDependencies": {
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@hapi/hapi": "^21.4.0",
    "@hapi/joi": "^17.1.1",
