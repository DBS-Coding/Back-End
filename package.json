{
  "name": "be",
  "module": "index.ts",
  "version": "0.0.1",
  "description": "A Bun package be",
  "main": "index.ts",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --watch src --exec tsx src/index.ts",
    "build": "tsc",
    "deploy": "bun run build && vercel --prod",
    "lint": "eslint .",
    "format": "prettier --write .",
    "git:push": "sh -c 'git add . && git commit -m \"$*\" && git push' dummy",
