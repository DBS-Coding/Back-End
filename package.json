    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.1.10",
    "pg": "^8.16.0",
    "ws": "^8.18.2",
    "zod": "^3.25.51"
