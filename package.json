    "db:push": "supabase db push --linked",
    "db:reset": "supabase db reset --linked"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@types/bun": "^1.2.13",
    "@types/hapi__hapi": "^21.0.0",
